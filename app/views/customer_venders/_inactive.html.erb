<div class="modal-body">
  <p><%= t "customer_vender.messages.inactive_confirm" %></p>
</div>
<div class="modal-footer">
  <button type="button" class="btn btn-default" data-dismiss="modal">
    <%= t "actions.close" %></button>
  <%= link_to t("customer_vender.actions.inactive"),
    partner_path(@status, @customer_vender, type: "inactive"), method: :delete,
    remote: true, class: "btn btn-warning" %>
</div>
