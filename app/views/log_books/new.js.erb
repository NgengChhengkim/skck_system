$(".logbook-modal").html("<%= j render 'log_books/form_modal', object: @log_book %>")
$(".logbook-form-modal").modal("show");

if($("#journal-entry").length > 0) {
  var modal_transaction_date = $("#log_book_transaction_date");
  modal_transaction_date.val($(".transaction-date").val());

  var cash_type_selectize = $(".cash_type")[0].selectize;
  var bank_type_selectize = $(".bank_type")[0].selectize;
  var cash_type_id = bank_type_selectize.options[bank_type_selectize.getValue()].cash_type
  cash_type_selectize.addItem(cash_type_id);
}
