<div id="journal-entry" class="row">
  <div class="col-md-9">
    <ol class="breadcrumb">
      <li><%= link_to "", root_path, class: "glyphicon glyphicon-home" %></li>
      <li class="active"><%= t "journal_entries.breadcrumbs.general_journal" %></li>
    </ol>
    <div class="journal-header">
      <span class="title"><%= t "journal_entries.title" %></span>
      <span class="btn-navigate pull-right">
        <%= link_to "", "javascript:void(0)", class: "btn btn-default btn-next glyphicon glyphicon-chevron-left" %>
        <%= link_to "", "javascript:void(0)", class: "btn btn-default btn-prev glyphicon glyphicon-chevron-right" %>
        <%= link_to t("journal_entries.actions.add_new"), new_journal_entry_path,
          class: "btn btn-primary" %>
      </span>
    </div>
    <hr>
    <div class="journal-entry">
      <%= render "journal_entry_data" %>
    </div>
  </div>
  <div class="col-md-3">
    <%= render "right_panel" %>
  </div>
  <div class="logbook-modal"></div>
</div>
