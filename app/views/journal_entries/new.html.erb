<div id="journal-entry" class="row new-journal">
  <div class="col-md-9">
    <ol class="breadcrumb">
      <li><%= link_to "", root_path, class: "glyphicon glyphicon-home" %></li>
      <li><%= link_to t("journal_entries.breadcrumbs.general_journal"), journal_entries_path %></li>
      <li class="active"><%= t "journal_entries.breadcrumbs.new" %></li>
    </ol>
    <div class="journal-header">
      <span class="title"><%= t "journal_entries.title" %></span>
      <span class="btn-navigate pull-right">
        <%= link_to t("journal_entries.actions.view_all"), journal_entries_path,
          class: "btn btn-default" %>
      </span>
    </div>
    <hr>
    <div class="journal-entry">
      <%= render "journal_entry", journal_entry: @journal_entry %>
    </div>
  </div>
  <div class="col-md-3">
    <%= render "right_panel" %>
  </div>
  <div class="logbook-modal"></div>
</div>
